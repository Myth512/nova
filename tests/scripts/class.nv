class Vector {
	def _init_(x, y, z) {
		self.x = x
		self.y = y
		self.z = z
	}

	def _str_() {
		return "x: {self.x}, y: {self.y}, z: {self.z}"
	}

	def _add_(other) {
		return Vector(self.x + other.x, self.y + other.y, self.z + other.z) 
	}
}

v1 := Vector(1, 2, 3)
v2 := Vector(4, 5, 6)

print(v1 + v2)
