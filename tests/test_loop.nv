i := 0
for i < 5 {
	print(i)
	i++
}

for x := -5; x < 0; x += 2 {
	print(x)
}

a := 0

for {
	a++
	print(a)
} (a < 3)

msg := "Hello, im string"

for {
	print(msg)
} (msg == "Hello string, im another string")

b := 0

for {
	b++
	if b % 20 != 0 {
		continue
	}
	if b >= 100 {
		break
	}
	print(b)
}

for y := 0; y < 2; y++ {
	for x := 0; x < 2; x++ {
		print(y, x)
	}
}

for j := 0; j <= 3; j++ {
	for k := j + 1; k <= 3; k++ {
		print(j, k)
	}
}

